{

        int i;
        int j;
        float v;
        float x;
        float[100] a;
        while(true)
        {
        do i=i+1;while(a[i]<v);

        do j=j-1;while(a[j]>v);
        if(i>=j)break;
        x=a[i];a[i]=a[j];a[j]=x;
        }
        }

        {
        int[10] a;
        a[1] = 1;
        if(a[1] == 1)
        a[1] = 2;
        a[1] = 3;
        }